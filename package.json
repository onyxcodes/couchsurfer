  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "start": "npm run build && npm run start:server",
    "start:server": "node ./server/lib/index.js",
    "develop:ui": "webpack serve --node-env=development --progress",
    "develop:server": "ts-node ./server/index.ts",
    "test": "jest --config jest.config.ts",
    "build:types": "npx tsc -p server/tsconfig.json",
    "build:server": "rollup --config server/rollup.config.js && npm run build:types && cp -r server/src/utils/dbManager/datamodel/patch lib/",
    "build:ui": "webpack --node-env=production",
    "build": "npm run build:server && npm run build:ui",
    "build-docs:server": "npx typedoc --options ./server/typedoc.json",
    "build-docs:ui": "npx typedoc --options ./ui/typedoc.json",
    "build-docs": "npm run build-docs:server && npm run build-docs:ui"
  },
